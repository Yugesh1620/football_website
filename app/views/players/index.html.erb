<h1>Players</h1>

<table class="table table-bordered">
  <tr>
    <th>#</th>
    <th>Name</th>
    <th>Age</th>
    <th>Country</th>
    <th>Matches</th>
    <th>Goals</th>
    <th>Assists</th>
    <th>Picture</th>
  </tr>
  <% @players.each do |player| %>
  <tr>
    <td><%= player.id %></td>
    <td><%= player.name %></td>
    <td><%= player.age %></td>
    <td><%= player.country %></td>
    <td><%= player.matches %></td>
    <td><%= player.goals %></td>
    <td><%= player.assists %></td>
    <td> <img src=<%=player.picture%> width="128" class="img-circle"></td>
  </tr>
  <% end %>



</table>
